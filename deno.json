{
  "name": "@fry69/aqfile",
  "version": "0.1.0",
  "exports": {
    ".": "./src/main.ts"
  },
  "license": "MIT",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check && deno task test",
    "test": "deno test -A --ignore=tests/e2e.test.ts",
    "test:e2e": "deno test -A tests/e2e.test.ts",
    "e2e": "deno run --env-file=./.env.e2e -A ./src/main.ts",
    "lex": "lex-cli generate -c ./lex.config.ts"
  },
  "imports": {
    "@atcute/lex-cli": "npm:@atcute/lex-cli@^2.2.2",
    "@atcute/lexicons": "npm:@atcute/lexicons@^1.2.2",
    "@atproto/api": "npm:@atproto/api@^0.17.2",
    "@std/cli": "jsr:@std/cli@^1.0.23",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fmt": "jsr:@std/fmt@^1.0.5",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/semver": "jsr:@std/semver@^1.0.3",
    "mime-types": "npm:mime-types@^3.0.1"
  }
}
